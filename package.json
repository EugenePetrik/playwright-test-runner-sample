{
  "name": "playwright-ts-playwright-test-runner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "codegen": "npx playwright codegen",
    "lint": "eslint --fix",
    "pretest": "npm run remove-logs",
    "test": "npx playwright test --reporter=list --project=chromium",
    "test:debug": "set PWDEBUG=1 && npm run test --reporter=line",
    "test:console": "set PWDEBUG=console && npm run test --reporter=line",
    "test:logs": "set DEBUG=pw:api && npm run test --reporter=line",
    "test:mobile": "npm run test --project=\"iPhone 11\" --reporter=dot,line",
    "remove-logs": "ts-node ./src/utils/logs.cleaner.ts"
  },
  "keywords": [
    "Playwright",
    "TypeScript",
    "Playwright/test",
    "Allure"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.13.0",
    "@types/faker": "^5.5.7",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.4.2",
    "@types/tough-cookie": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "dotenv": "^10.0.0",
    "envalid": "^7.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-playwright": "^0.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "got": "^11.8.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "playwright": "^1.13.0",
    "prettier": "^2.3.2",
    "tough-cookie": "^4.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "winston": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --debug --fix",
      "prettier --write --ignore-unknown"
    ]
  }
}
